{
  "presets": ["es2015", "stage-0", "react"],
  "plugins": [
           ["import", [{"libraryName": "antd", "style": "css"}]],
           ["syntax-dynamic-import"]
  ],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}